{
  "name": "stackjanitor",
  "version": "1.25.0",
  "description": "StackJanitor cleans up CloudFormation stacks based on TTL and tags.",
  "main": "src/handler.ts",
  "private": true,
  "author": "Lendi",
  "license": "MIT",
  "repository": "git@github.com:lendi-au/StackJanitor.git",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules .serverless lib",
    "format": "prettier --write '**/*.{md,json,js,jsx,ts,tsx}'",
    "lint:format": "prettier-check '**/*.{md,json,js,jsx,ts,tsx}'",
    "test": "jest",
    "package": "serverless package",
    "deploy": "serverless deploy",
    "release": "release-it minor --ci --no-git.requireUpstream"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.387.0",
    "@aws-sdk/client-dynamodb": "^3.387.0",
    "@aws-sdk/client-lambda": "^3.387.0",
    "@aws-sdk/util-dynamodb": "^3.387.0",
    "dynogels": "^9.1.0",
    "pino": "^5.14.0",
    "pino-pretty": "^3.5.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/dynogels": "^9.0.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.16",
    "@types/pino": "^5.14.0",
    "@types/sinon": "^10.0.2",
    "aws-sdk-client-mock": "^3.0.0",
    "graceful-fs": "^4.2.3",
    "husky": "^1.3.1",
    "jest": "^29.6.2",
    "lint-staged": "^8.1.4",
    "prettier": "1.19.1",
    "prettier-check": "^2.0.0",
    "release-it": "^14.11.7",
    "serverless": "^3.34.0",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-step-functions": "^3.14.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^10.9.1",
    "tslint": "^5.12.1",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "**/tar-stream": "^2.0.0",
    "**/bl": "^4.0.3"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "d.ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
