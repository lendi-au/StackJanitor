{
  "name": "stackjanitor",
  "version": "1.17.0",
  "description": "StackJanitor cleans up CloudFormation stacks based on TTL and tags.",
  "main": "src/handler.js",
  "private": true,
  "author": "Lendi",
  "license": "MIT",
  "repository": "git@github.com:lendi-au/StackJanitor.git",
  "scripts": {
    "format": "prettier --write '**/*.{md,json,js,jsx,ts,tsx}'",
    "lint:format": "prettier-check '**/*.{md,json,js,jsx,ts,tsx}'",
    "test": "jest",
    "package": "serverless package",
    "deploy": "serverless deploy",
    "release": "release-it minor --ci --no-git.requireUpstream"
  },
  "dependencies": {
    "dynogels": "^9.1.0",
    "pino": "^5.14.0",
    "pino-pretty": "^3.5.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@hapi/joi": "^15.1.1",
    "@types/aws-lambda": "^8.10.37",
    "@types/dynogels": "^9.0.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.16",
    "@types/pino": "^5.14.0",
    "aws-sdk": "^2.502.0",
    "graceful-fs": "^4.2.3",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.4",
    "prettier": "1.19.1",
    "prettier-check": "^2.0.0",
    "release-it": "^13.6.7",
    "serverless": "^1.82.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-step-functions": "^2.23.0",
    "serverless-webpack": "^5.3.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.5.4",
    "tslint": "^5.12.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2"
  },
  "resolutions": {
    "**/tar-stream": "^2.0.0",
    "**/bl": "^4.0.3"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "d.ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
